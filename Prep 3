        "# Insert sample NFL team data\n",
        "cursor.execute(\"\"\"\n",
        "INSERT INTO scouting_stats (team_name, city, wins, points_per_game)\n",
        "VALUES\n",
        "    ('Chiefs', 'Kansas City', 11, 29.2),\n",
